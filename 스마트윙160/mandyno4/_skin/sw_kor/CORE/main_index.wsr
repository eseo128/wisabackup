        gauge3.setValueAnimated(value3, 1.5);
        gauge4.setValueAnimated(value4 * 300, 2);
        gauge5.setValueAnimated(value5 * 2, 1);
        gauge6.setValueAnimated(value1, 1);
        gauge7.setValueAnimated(value1, 1);
        window.setTimeout(loop, 4000);
        })();
