	<script>
	// delay 없이 서서히 움직이는 가로 롤링 배너
	window.onload = function() {
            var bannerLeft=0;
            var first=1;
            var last;
            var slideCnt=0;
            var $slide = $(".text_slide .slide span");
            var $first;
            var $last;

            $slide.each(function(){   // 5px 간격으로 배너 처음 위치 시킴
                $(this).css("left",bannerLeft);
                bannerLeft += $(this).width()+5;
                $(this).attr("id", "banner"+(++slideCnt));  // slide에 id 속성 추가
            });

            if( slideCnt > 1){                //배너 n개 이상이면 이동시킴
                last = slideCnt;
                setInterval(function() {
                    $slide.each(function(){
                        $(this).css("left", $(this).position().left-1); // 1px씩 왼쪽으로 이동
                    });
                    $first = $("#banner"+first);
                    $last = $("#banner"+last);
                    if($first.position().left < -1000) {    // 제일 앞에 배너 제일 뒤로 옮김 (조절 필요)
                        $first.css("left", $last.position().left + $last.width()+5 );
                        first++;
                        last++;
                        if(last > slideCnt) { last=1; }   
                        if(first > slideCnt) { first=1; }
                    }
                }, 1);   //여기 값을 조정하면 속도를 조정할 수 있다.(위에 1px 이동하는 부분도 조정하면 깔끔하게 변경가능하다           
 			}
	};

	</script>
