				 numberCounting('count_con3', 5678);
				 function numberCounting(countCon, number) {
					 var memberCountConTxt= number;
					 $({ val : 0 }).animate({ val : memberCountConTxt }, {
						 duration: 5000,
						 step: function() {
							 var num = numberWithCommas(Math.floor(this.val));
							 $("."+countCon).text(num);
						 },
						 complete: function() {
							 var num = numberWithCommas(Math.floor(this.val));
							 $("."+countCon).text(num);
						 }
					 });
				 }
				 function numberWithCommas(x) {
